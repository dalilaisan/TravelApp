<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ManageInterestsActivity">

    <ImageView
        android:id="@+id/hospital"
        android:layout_width="96dp"
        android:layout_height="96dp"
        android:layout_marginTop="24dp"
        android:baselineAlignBottom="true"
        app:layout_constraintStart_toStartOf="@+id/caffee"
        app:layout_constraintTop_toBottomOf="@+id/caffee"
        app:srcCompat="@drawable/hospital_256px" />

    <ImageView
        android:id="@+id/bank"
        android:layout_width="96dp"
        android:layout_height="96dp"
        android:layout_marginLeft="24dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        android:baselineAlignBottom="true"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:srcCompat="@drawable/button" />

    <ImageView
        android:id="@+id/drink"
        android:layout_width="96dp"
        android:layout_height="94dp"
        android:layout_alignBaseline="@+id/bank"
        android:layout_marginLeft="24dp"
        android:layout_marginStart="24dp"
        android:baselineAlignBottom="true"
        app:layout_constraintBaseline_toBaselineOf="@+id/bank"
        app:layout_constraintStart_toEndOf="@+id/bank"
        app:srcCompat="@drawable/bar_256px" />

    <ImageView
        android:id="@+id/caffee"
        android:layout_width="96dp"
        android:layout_height="96dp"
        android:layout_marginTop="24dp"
        android:baselineAlignBottom="true"
        app:layout_constraintStart_toStartOf="@+id/bank"
        app:layout_constraintTop_toBottomOf="@+id/bank"
        app:srcCompat="@drawable/cafe_256px" />

    <ImageView
        android:id="@+id/hotel"
        android:layout_width="95dp"
        android:layout_height="96dp"
        android:layout_marginLeft="24dp"
        android:layout_marginStart="24dp"
        android:baselineAlignBottom="true"
        app:layout_constraintBaseline_toBaselineOf="@+id/eat"
        app:layout_constraintStart_toEndOf="@+id/eat"
        app:srcCompat="@drawable/hotel_256px" />

    <ImageView
        android:id="@+id/eat"
        android:layout_width="96dp"
        android:layout_height="96dp"
        android:layout_alignBaseline="@+id/caffee"
        android:layout_marginLeft="24dp"
        android:layout_marginStart="24dp"
        android:baselineAlignBottom="true"
        app:layout_constraintBaseline_toBaselineOf="@+id/caffee"
        app:layout_constraintStart_toEndOf="@+id/caffee"
        app:srcCompat="@drawable/restaurant_256px" />

    <ImageView
        android:id="@+id/shopping"
        android:layout_width="96dp"
        android:layout_height="96dp"
        android:layout_marginLeft="24dp"
        android:layout_marginStart="24dp"
        android:baselineAlignBottom="true"
        app:layout_constraintBaseline_toBaselineOf="@+id/drink"
        app:layout_constraintStart_toEndOf="@+id/drink"
        app:srcCompat="@drawable/shopping_256px" />

    <ImageView
        android:id="@+id/transport"
        android:layout_width="96dp"
        android:layout_height="96dp"
        android:layout_marginLeft="144dp"
        android:layout_marginStart="144dp"
        android:baselineAlignBottom="true"
        app:layout_constraintBaseline_toBaselineOf="@+id/hospital"
        app:layout_constraintStart_toEndOf="@+id/hospital"
        app:srcCompat="@drawable/transport_256px" />

    <Button
        android:id="@+id/doneButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="24dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:text="Done!"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="What are you interested in?"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="24dp"
        android:text="Select what you like"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

</android.support.constraint.ConstraintLayout>